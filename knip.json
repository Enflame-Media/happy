{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "expo": {
    "config": ["app.json", "app.config.js"],
    "production": ["sources/app/**/*.{ts,tsx}"]
  },
  "entry": [
    "sources/app/**/*.{ts,tsx}",
    "plugins/**/*.js"
  ],
  "project": ["sources/**/*.{ts,tsx}", "plugins/**/*.js"],
  "ignore": [
    "sources/-*/**",
    "sources/trash/**",
    "sources/**/__testdata__/**",
    "sources/**/*.test.ts",
    "sources/**/*.spec.ts",
    "sources/**/*.appspec.ts",
    "sources/**/*.web.tsx",
    "sources/**/*.web.ts",
    "sources/**/*.ios.tsx",
    "sources/**/*.ios.ts",
    "sources/**/*.android.tsx",
    "sources/**/*.android.ts",
    "sources/**/*.native.ts",
    "sources/**/*.native.tsx",
    "sources/components/AgentContentView.tsx",
    "sources/components/OAuthView.tsx",
    "sources/utils/oauth.ts",
    "sources/components/autocomplete/**"
  ],
  "ignoreDependencies": [
    "@babel/runtime",
    "@elevenlabs/*",
    "@expo/*",
    "@happy/errors",
    "@happy/protocol",
    "@livekit/*",
    "@lottiefiles/*",
    "@react-native*",
    "@react-navigation/*",
    "@revenuecat/*",
    "@shopify/*",
    "@tauri-apps/*",
    "@types/*",
    "axios",
    "babel-*",
    "diff",
    "expo",
    "expo-*",
    "fuse.js",
    "knip",
    "libsodium-wrappers",
    "livekit-client",
    "lottie-react-native",
    "oxlint",
    "patch-package",
    "posthog-react-native",
    "qrcode",
    "react",
    "react-dom",
    "react-native",
    "react-native-*",
    "react-syntax-highlighter",
    "react-test-renderer",
    "react-textarea-autosize",
    "rn-encryption",
    "tsx",
    "typescript",
    "vitest",
    "wrangler",
    "zod",
    "zustand"
  ],
  "ignoreBinaries": [
    "eas",
    "expo",
    "knip",
    "oxlint",
    "patch-package",
    "tsc",
    "tsx",
    "vitest",
    "wrangler"
  ],
  "ignoreUnresolved": ["metro-minify-terser"],
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  }
}
